<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

<bean id="loginController" class="com.it.login.LoginController">
<constructor-arg name="memberDaoImpl" ref="memberDaoImpl"/>	
</bean>

<bean id="androidLoginController" class="com.it.login.AndroidLoginController">
<constructor-arg name="memberDaoImpl" ref="memberDaoImpl"/>	
</bean>

<bean id="androidFriendListController" class="com.it.friendlist.AndroidFriendListController">
<constructor-arg name="memberDaoImpl" ref="memberDaoImpl"/>	
</bean>

</beans>
